{
  "name": "@nuintun/svgo-loader",
  "version": "0.5.1",
  "description": "svgo loader for webpack.",
  "type": "module",
  "main": "cjs/index.cjs",
  "module": "esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.cjs",
    "default": "./esm/index.js"
  },
  "files": [
    "cjs",
    "esm"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuintun/svgo-loader.git"
  },
  "keywords": [
    "svgo",
    "loader",
    "rspack",
    "webpack"
  ],
  "author": {
    "name": "nuintun",
    "email": "nuintun@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuintun/svgo-loader/issues"
  },
  "homepage": "https://github.com/nuintun/svgo-loader#readme",
  "scripts": {
    "lint": "tsc --noEmit",
    "clean": "node tools/clean.js",
    "prepublishOnly": "pnpm build",
    "build:cjs": "rollup -c tools/rollup.cjs.js",
    "build:esm": "rollup -c tools/rollup.esm.js",
    "build:fix-types": "node tools/fix-types.js",
    "build": "pnpm clean && pnpm build:main && pnpm format",
    "format": "prettier --write . --ignore-path .prettierignore",
    "build:main": "pnpm build:cjs && pnpm build:esm && pnpm build:fix-types"
  },
  "dependencies": {
    "svgo": "^4.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@rspack/core": "^1.4.4",
    "dts-paths": "^1.1.9",
    "magic-string": "^0.30.17",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.44.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9"
  },
  "peerDependencies": {
    "@rspack/core": "^1.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  }
}
